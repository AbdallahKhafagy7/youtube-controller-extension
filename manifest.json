{
  "manifest_version": 3,
  "name": "YouTube Controller",
  "version": "1.0",
  "description": "Control YouTube video with ease.",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "permissions": []
}